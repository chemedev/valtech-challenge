{
  "name": "node-cookie-fortune",
  "vendor": "valtech",
  "version": "0.0.1",
  "title": "node-cookie-fortune",
  "description": "node-cookie-fortune",
  "dependencies": {},
  "builders": {
    "docs": "0.x",
    "graphql": "1.x",
    "node": "6.x"
  },
  "credentialType": "absolute",
  "policies": [
    { "name": "ADMIN_DS" },
    { "name": "outbound-access", "attrs": { "host": "api.vtex.com" } },
    { "name": "colossus-fire-event" },
    { "name": "colossus-write-logs" },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    }
  ],
  "settingsSchema": {
    "title": "admin/settings.title",
    "description": "admin/settings.description",
    "type": "object",
    "properties": {
      "appKey": {
        "type": "string",
        "title": "admin/settings.appKey"
      },
      "appToken": {
        "type": "string",
        "title": "admin/settings.appToken"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
